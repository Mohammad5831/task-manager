        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}"
          }
        ],
        "url": {
          "raw": "http://localhost:5000/api/auth/profile",
          "host": [
            "http://localhost:5000"
          ],
          "path": [
            "api",
            "auth",
            "profile"
          ]
        }
      }
    },
